.bar-container{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 300rpx;
  position: fixed;
  bottom: 0;
  background-color: white;
}

.bar-item{
  width: 65px;
  height: 65px;
  border-radius: 50%;
  border:1px solid black;
  text-align: center;
}

.bar-item text{
  line-height: 60px;
}